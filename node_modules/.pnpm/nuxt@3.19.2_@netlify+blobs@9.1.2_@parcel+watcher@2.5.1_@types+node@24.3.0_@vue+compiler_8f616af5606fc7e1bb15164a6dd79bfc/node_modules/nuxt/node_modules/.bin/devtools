#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.5_@types+node@24.3.0_jiti@2.5.1_terser@5.43.1_yaml@2.8.1__vue@3.5.21_typescript@5.9.2_/node_modules/@nuxt/devtools/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.5_@types+node@24.3.0_jiti@2.5.1_terser@5.43.1_yaml@2.8.1__vue@3.5.21_typescript@5.9.2_/node_modules/@nuxt/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.5_@types+node@24.3.0_jiti@2.5.1_terser@5.43.1_yaml@2.8.1__vue@3.5.21_typescript@5.9.2_/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.5_@types+node@24.3.0_jiti@2.5.1_terser@5.43.1_yaml@2.8.1__vue@3.5.21_typescript@5.9.2_/node_modules/@nuxt/devtools/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.5_@types+node@24.3.0_jiti@2.5.1_terser@5.43.1_yaml@2.8.1__vue@3.5.21_typescript@5.9.2_/node_modules/@nuxt/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/@nuxt+devtools@2.6.3_vite@7.1.5_@types+node@24.3.0_jiti@2.5.1_terser@5.43.1_yaml@2.8.1__vue@3.5.21_typescript@5.9.2_/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@nuxt+devtools@2.6.3_vite@7.1.5_@types+node@24.3.0_jiti@2.5.1_terser@5.43.1_yaml@2.8.1__vue@3.5.21_typescript@5.9.2_/node_modules/@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../@nuxt+devtools@2.6.3_vite@7.1.5_@types+node@24.3.0_jiti@2.5.1_terser@5.43.1_yaml@2.8.1__vue@3.5.21_typescript@5.9.2_/node_modules/@nuxt/devtools/cli.mjs" "$@"
fi
