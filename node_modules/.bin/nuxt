#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/nuxt@3.19.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler_8f616af5606fc7e1bb15164a6dd79bfc/node_modules/nuxt/bin/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/nuxt@3.19.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler_8f616af5606fc7e1bb15164a6dd79bfc/node_modules/nuxt/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/nuxt@3.19.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler_8f616af5606fc7e1bb15164a6dd79bfc/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/nuxt@3.19.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler_8f616af5606fc7e1bb15164a6dd79bfc/node_modules/nuxt/bin/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/nuxt@3.19.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler_8f616af5606fc7e1bb15164a6dd79bfc/node_modules/nuxt/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/nuxt@3.19.2_@netlify+blobs@9.1.2_@parcel+watcher@2.5.1_@types+node@24.3.0_@vue+compiler_8f616af5606fc7e1bb15164a6dd79bfc/node_modules:/Users/sandhyagautam/Desktop/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
